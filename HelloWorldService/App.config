<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="ImageDirPath" value="c:/temp/images/"/>
    <add key="CacheSize" value="5"/>
    <add key="Throw" value="true"/>
  </appSettings>
  <system.serviceModel>
    <bindings>
      <wsHttpBinding>
        <binding name="WSHttpBinding_IImageService"/>
      </wsHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:8088/ServiceModelSamples/imageservice" binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IImageService" contract="ServiceReference1.IImageService" name="WSHttpBinding_IImageService">
        <identity>
          <servicePrincipalName value="host/WIN-7H86A959HSM"/>
        </identity>
      </endpoint>
    </client>
    <services>
      <!-- This section is optional with the new configuration model
           introduced in .NET Framework 4. -->
      <service name="HelloWorldService.wcfservice.ImageService" behaviorConfiguration="ImageServiceBehavior">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8088/ServiceModelSamples/imageservice"/>
          </baseAddresses>
        </host>
        <!-- this endpoint is exposed at the base address provided by host: http://localhost:8088/ServiceModelSamples/service  -->
        <endpoint address="" binding="wsHttpBinding" contract="HelloWorldService.wcfservice.IImageService"/>
        <!-- the mex endpoint is exposed at http://localhost:8088/ServiceModelSamples/imageservice/mex -->
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="ImageServiceBehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="True"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
